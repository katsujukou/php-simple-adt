{
    "name": "katsujukou/php-simple-adt",
    "description": "A simple algebraic data types (a.k.a. ADT) library based on PHP's subtyping polymorphism",
    "type": "library",
    "version": "0.1.0",
    "scripts": {
        "post-install-cmd": "\\SimpleADT\\Command\\Install::postInstall"
    },
    "require": {
        "php": "^7.2.0|^8.0",
        "nikic/php-parser": "^4.10",
        "ext-json": "*",
        "phpstan/phpdoc-parser": "^0.5.3",
        "php-parallel-lint/php-console-color": "^1.0",
        "phpstan/phpstan": "^0.12.83"
    },
    "autoload": {
        "psr-4": {
            "SimpleADT\\": "src/"
        },
        "classmap": [
            "output"
        ]
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/phalg"
    ]    
}
