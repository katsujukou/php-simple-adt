{
    "name": "katsujukou/php-simple-adt",
    "description": "A simple algebraic data types (a.k.a. ADT) library based on PHP's subtyping polymorphism",
    "type": "library",
    "scripts": {
        "pre-autoload-dump": [
            "mkdir -p ./output"
        ]
    },
    "require": {
        "php": "^7.2.0|^8.0",
        "nikic/php-parser": "^4.10",
        "phpstan/phpdoc-parser": "0.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "SimpleADT\\": "src/"
        },
        "classmap": [
            "output"
        ]
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/phalg"
    ]    
}
